{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "node",
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@local_modules/*": ["./src/local_modules/*"],
      "@env": ["./src/env.js"]
    },
    "outDir": "./dist"
  }
}
